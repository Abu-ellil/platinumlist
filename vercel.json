{
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 60
    },
    "app/api/**/*.mjs": {
      "runtime": "nodejs20.x",
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
